            <div class="item active">
                <img src="~/Image/img10.jpg" alt="vill1" style="width:100%;height:auto;" />
            </div>

            <div class="item">
                <img src="~/Image/img1.jpg" alt="vill2" style="width:100%;height:auto;" />
